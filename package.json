{
  "name": "casbin-client",
  "version": "0.2.0",
  "type": "module",
  "types": "src",
  "description": "Lightweight client for Casbin",
  "sideEffects": false,
  "repository": {
    "url": "git+https://github.com/Raiondesu/casbin-client.git"
  },
  "author": {
    "name": "Raiondesu",
    "url": "https://github.com/raiondesu"
  },
  "module": "dist/index.js",
  "devDependencies": {
    "@types/bun": "latest",
    "badge-maker": "^5.0.2",
    "coverage-percentage": "^0.0.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "module-sync": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./src/core.ts",
      "module-sync": "./dist/core.js",
      "import": "./dist/core.js"
    },
    "./model": {
      "types": "./src/model.ts",
      "module-sync": "./dist/model.js",
      "import": "./dist/model.js"
    },
    "./policy": {
      "types": "./src/policy.ts",
      "module-sync": "./dist/policy.js",
      "import": "./dist/policy.js"
    },
    "./parser": {
      "types": "./src/parser.ts",
      "module-sync": "./dist/parser.js",
      "import": "./dist/parser.js"
    },
    "./min": {
      "types": "./src/index.ts",
      "module-sync": "./dist/min/index.js",
      "import": "./dist/min/index.js"
    },
    "./min/core": {
      "types": "./src/core.ts",
      "module-sync": "./dist/min/core.js",
      "import": "./dist/min/core.js"
    },
    "./min/model": {
      "types": "./src/model.ts",
      "module-sync": "./dist/min/model.js",
      "import": "./dist/min/model.js"
    },
    "./min/policy": {
      "types": "./src/policy.ts",
      "module-sync": "./dist/min/policy.js",
      "import": "./dist/min/policy.js"
    },
    "./min/parser": {
      "types": "./src/parser.ts",
      "module-sync": "./dist/min/parser.js",
      "import": "./dist/min/parser.js"
    }
  },
  "keywords": [
    "casbin",
    "casbin.js",
    "permissions",
    "rbac",
    "abac",
    "access-control",
    "access",
    "casbin-frontend",
    "authz",
    "authorization",
    "auth"
  ],
  "scripts": {
    "build": "bun scripts/build.ts",
    "test": "bun test && bun scripts/badges.ts"
  },
  "dependencies": {
    "subscript": "^9.1.0"
  }
}
